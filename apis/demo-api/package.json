{
  "name": "demo-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./src",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d src/data-source",
    "migration:create": "pnpm run typeorm migration:generate ./src/migrations/CreateDB",
    "migration:run": "pnpm run typeorm migration:run",
    "seed": "NODE_OPTIONS=--unhandled-rejections=strict TS_NODE_LOG_ERROR=true npx ts-node --logError --pretty ./src/seeding/index.ts",
    "seed:teams": "pnpm run seed teams-seed",
    "seed:players": "pnpm run seed players-seed"
  },
  "dependencies": {
    "types": "*",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express-session": "^1.17.3",
    "find-up": "^6.3.0",
    "node-fetch": "2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "csvtojson": "^2.0.10"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.1.3",
    "@types/node": "18.11.18",
    "eslint": "8.32.0",
    "eslint-config-custom": "*",
    "jest": "29.3.1",
    "nest-commander": "^3.4.0",
    "source-map-support": "^0.5.20",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
